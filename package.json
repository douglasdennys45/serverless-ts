{
  "name": "node-ts",
  "version": "0.0.0",
  "description": "Node.JS + TypeScript",
  "main": "build/main.js",
  "license": "MIT",
  "scripts": {
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "check": "npm-check -s -u",
    "clean": "rimraf build",
    "dev": "ts-node-dev --respawn --transpile-only --clear src/main.ts",
    "lint": "eslint ./src",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky install",
    "test": "jest --passWithNoTests --no-cache --runInBand --forceExit",
    "test:staged": "npm run test -- --findRelatedTests",
    "test:coverage": "npm run test -- --coverage"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "module-alias": "^2.2.2",
    "mongodb": "^4.10.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@jest-mock/express": "^2.0.1",
    "@shelf/jest-mongodb": "^4.1.1",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^29.1.2",
    "@types/module-alias": "^2.0.1",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^18.11.0",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "eslint": "^8.25.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-promise": "^6.1.0",
    "husky": "^8.0.1",
    "jest": "^29.2.0",
    "jest-mock-extended": "^3.0.1",
    "lint-staged": "^13.0.3",
    "serverless-azure-functions": "^2.1.3",
    "serverless-dotenv-plugin": "^4.0.2",
    "stub-azure-function-context": "^2.0.0-alpha.7",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4"
  },
  "keywords": [
    "Node.JS",
    "TypeScript",
    "Jest"
  ],
  "author": {
    "name": "Douglas Dennys",
    "email": "douglasdennys45@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/douglasdennys45/node-ts.git"
  },
  "bugs": {
    "url": "https://github.com/douglasdennys45/node-ts/issues",
    "email": "douglasdennys45@gmail.com"
  }
}
